{
  "compilerOptions": {
    "target": "es2021",
    "module": "es2022",
    "moduleResolution": "node",
    "lib": ["es2021"],
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "strict": true,
    "skipLibCheck": true,
    "noUncheckedIndexedAccess": true,
    "outDir": "./dist",
    "noEmit": true,
    "types": [
      "@cloudflare/workers-types"
    ],
    "baseUrl": "./",
    "paths": {
      "@standev2001/db_connection/types":["node_modules/@standev2001/db_connection/dist/types"],
      "@standev2001/db_connection/utils":["node_modules/@standev2001/db_connection/dist/utils"],
    }
  }
}
